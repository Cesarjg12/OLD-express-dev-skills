<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Skills</title>
</head>
<body>
    <ul>
      <% skills.forEach(function(t) { %>
        <li>
            <a href="/skills/<%= t.id %>"><%= t.chores %></a>
            - 
          <%= t.done ? 'done' : 'not done' %>
        </li>
      <% }); %>
    </ul>
    
</body>
</html>